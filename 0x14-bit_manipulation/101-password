#include <stdio.h>
#include <string.h>

/**
 * main - Entry point of the program
 *
 * Return: 0 on success, non-zero on error
 */
int main(void)
{
char password[20];

printf("Enter the password: ");
scanf("%s", password);

if (strcmp(password, "Hol") == 0
{
printf("Congratulations!\n");
}
else
{
printf("Invalid password.\n");
}

return (0);
}

